<div class="row">
    <div class="col-md-8">
      <app-file-upload 
        projectId="100" 
        sectionId="107" 
        maxFiles="4"
        maxSize="2"
        fileExt="XML"
        (uploadStatus)="refreshImages($event)" >
      </app-file-upload>
    </div>
    <div class="col-md-4">
      <input class="export" type="button" value="Export to XML" (click) = "ExportRecords()">
        
    </div>
   
  </div>

  <hr>


